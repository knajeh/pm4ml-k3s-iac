project=pm4mladdons
cloud_provider=aws
use_bastion=true
create_public_zone=yes
create_private_zone=yes
master_instance_type=t3.medium
master_volume_size=100
agent_node_count=1
agent_instance_type=m5.large
agent_volume_size=100
ingress_name=nginx
letsencrypt_email=cicd@modusbox.com
install_mojaloop=no
install_pm4ml=yes
wireguard_client_count=20
environment=k3s
monitoring_stack=loki
client=##client####environment##k3s
domain=##domain##
region=##region##
install_sims=no
pm4ml_config_file=/k3s-boot/ansible_pm4ml_output.yaml
pm4ml_static_config_file=/k3s-boot/static.pm4ml.values.yaml
pm4ml_client_cert_remote_dir=/tmp/pm4ml-certoutput/
pm4ml_client_cert_local_dir=##pm4ml_client_cert_local_dir##
pm4ml_helm_version=7.0.7
pm4ml_dfsp_internal_access_only=yes
letsencrypt_server=production
internal_pm4ml_instance=yes
peer_vpc_id=##peer_vpc_id##
create_vpc_peering=true
vpc_cidr=10.107.0.0/23
k3s_version=v1.20.8+k3s1
install_portainer=no
internal_managed_tls=yes
extra_tag_file=/k3s-boot/extra-tags.json
ext_gw_listen_port=443
use_aws_acm_cert=false
aws_acm_wildcard_entry=pm4mlname
grafana_internal_access_only=yes
az_count=1