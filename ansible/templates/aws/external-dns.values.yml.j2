provider: aws
aws:
  credentials:
    accessKey: {{ external_dns_iam_access_key }}
    secretKey: {{ external_dns_iam_secret_key }}
  region: {{ region }}
domainFilters:
  - {{ domain }}
  - {{ domain }}.internal
txtOwnerId: {{ client }}
policy: upsert-only
dryRun: false
interval: 30m
triggerLoopOnEvent: true